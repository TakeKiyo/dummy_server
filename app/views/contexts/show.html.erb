<div>
  <strong>入力</strong>
  <%= @context.input %>
  <% inp = @context.input %>
  <br>
  <br>
  <table border="1">
    <tr>
      <th>順列</th>
      <th>逆数</th>
    </tr>
      <% arr = inp.split('') %>
      <% comb_list = arr.permutation(arr.size).to_a %>
      <% comb_list.each do |comb| %>
        <% str = '' %>
        <% comb.each do |moji| %>
          <% str += moji %>
        <% end %>
        <tr>
          <th><%= str %></th>
          <th> 1 / <%= str %></th>
        </tr>
      <% end %>

  </<table>
  <table>
</<table>
</div>
<br>
<%= link_to 'いままでの実行一覧', contexts_path %>
